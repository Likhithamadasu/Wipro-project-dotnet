@model List<EmployeeFormApp.Models.EmployeeForm>

@{
    ViewData["Title"] = "Employee List";
}

<h2>Employee List</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>Date of Birth</th>
            <th>Country</th>
            <th>Email ID</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.FirstName</td>
                <td>@employee.LastName</td>
                <td>@employee.Age</td>
                <td>@employee.DateOfBirth.ToShortDateString()</td>
                <td>@employee.Country</td>
                <td>@employee.EmailId</td>
                <td>@employee.Gender</td>
            </tr>
        }
    </tbody>
</table>
